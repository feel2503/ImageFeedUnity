<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.feed.plugin">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name=".ImgSelectActivity"
            android:theme="@style/AppTheme_NoTitle">
            <!--<intent-filter>-->
                <!--<action android:name="android.intent.action.MAIN" />-->
                <!--<category android:name="android.intent.category.LAUNCHER" />-->
            <!--</intent-filter>-->
        </activity>
        <activity
            android:name=".ImgEditActivity"
            android:theme="@style/AppTheme_NoTitle" />
        <activity
            android:name=".ImgFilterActivity"
            android:theme="@style/AppTheme_NoTitle" />
        <activity
            android:name=".FeedUploadActivity"
            android:theme="@style/AppTheme_NoTitle"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".ImageViewActivity"
            android:theme="@style/AppTheme_NoTitle" />
        <activity
            android:name=".NoticeDialog"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name=".FacePhotoActivity"
            android:theme="@style/AppTheme_NoTitle">

             <!--<intent-filter>-->
             <!--<action android:name="android.intent.action.MAIN" />-->
             <!--<category android:name="android.intent.category.LAUNCHER" />-->
             <!--</intent-filter>-->

        </activity>
        <activity
            android:name=".FaceGalleryActivity"
            android:theme="@style/AppTheme_NoTitle" />

        <activity android:name=".ProfileActivity"
            android:theme="@style/AppTheme_NoTitle" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>